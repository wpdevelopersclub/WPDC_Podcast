{
    "name" 			: "wpdevelopersclub/wpdevsclub-podcast",
    "description" 	: "WP Developers Club Podcast Plugin",
    "type"          : "project",
    "keywords" 		: ["wordpress", "theme", "wp", "plugin"],
    "homepage" 		: "http://wpdc.dev",
    "license" 		: "GPL-2.0+",
    "authors": [
        {
            "name" 	: "Tonya",
            "email" : "hello@wpdc.dev"
        }
    ],
    "require": {
        "php" : ">=5.4.0"
    },
    "autoload" : {
        "classmap" : [
            "class-podcast.php",

            "src/template-classes/class-page.php",
            "src/template-classes/class-podcast-landing.php",
            "src/template-classes/class-single.php"
        ],
        "files": []
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "vendor-dir": "assets/vendor"
    },
    "minimum-stability": "dev"
}